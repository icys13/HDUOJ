#include <stdio.h>

int main()
{
    int N, m , n, min, x;
    scanf ("%d", &N);
    while (N--)
    {
        min = 101;
        scanf ("%d%d", &n, &m);
        while (n--)
        {
            scanf("%d", &x);
            if (x < min)
                min = x;
        }
        printf("%d\n", (100 - min) * (100 - min));
    }
    return 0;
}
